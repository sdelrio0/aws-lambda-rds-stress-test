{
  "name": "doorbell",
  "version": "0.1.0",
  "description": "Test the limits of the Lambda-RDS relationship.",
  "main": "index.js",
  "scripts": {
    "build:clean": "rm -rf dist/",
    "prebuild": "npm run build:clean",
    "build": "babel --copy-files --out-dir dist/ ./index.js # > /dev/null",
    "test": "echo \"Error: no test specified\" && exit 1 # source .env.test && mocha --compilers js:babel-core/register --require ./__tests__/test_helper.js --recursive ./__tests__/tests",
    "start": "npm run build && node ./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sdelrio0/doorbell.git"
  },
  "keywords": [
    "aws",
    "lambda",
    "rds",
    "stress",
    "test",
    "limit",
    "experiment"
  ],
  "author": "Sergio del Rio @sdelrio0",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sdelrio0/doorbell/issues"
  },
  "homepage": "https://github.com/sdelrio0/doorbell#readme",
  "dependencies": {
    "aws-sdk": "^2.7.21"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-2": "^6.18.0",
    "chai": "^3.5.0",
    "mocha": "^3.2.0"
  }
}
